{
  "name": "superbowleto",
  "version": "1.0.0",
  "description": "A simple microservice to issue, register and manage boletos",
  "main": "index.js",
  "private": "true",
  "scripts": {
    "build-production": "webpack --config ./webpackfile.production.js",
    "build-test": "webpack --config ./webpackfile.test.js",
    "lint": "eslint 'src/**/*.js' 'test/**/*.js'",
    "setup": "node -r babel-register ./script/setup",
    "test": "ava --verbose '__tests__/test/**/*.js'",
    "test-functional": "ava --verbose '__tests__/test/functional/**/*.js'",
    "test-integration": "ava --verbose '__tests__/test/integration/**/*.js'",
    "test-unit": "ava --verbose '__tests__/test/unit/**/*.js'",
    "pretest": "npm run setup && npm run build-test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pagarme/superbowleto.git"
  },
  "keywords": [
    "boleto",
    "microservice"
  ],
  "author": "Pagar.me Pagamentos S/A <@pagarme>",
  "bugs": {
    "url": "https://github.com/pagarme/superbowleto/issues"
  },
  "homepage": "https://github.com/pagarme/superbowleto#readme",
  "dependencies": {
    "aws-sdk": "2.25.0",
    "bluebird": "3.4.7",
    "cuid": "1.3.8",
    "joi": "10.2.2",
    "pg": "6.1.2",
    "ramda": "0.23.0",
    "sequelize": "3.30.0",
    "serverless": "1.6.1",
    "sqs-quooler": "1.1.2"
  },
  "devDependencies": {
    "@ava/babel-preset-stage-4": "1.0.0",
    "ava": "0.18.2",
    "babel-loader": "6.4.1",
    "babel-plugin-transform-builtin-extend": "1.1.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-es2015": "6.22.0",
    "chai": "3.5.0",
    "chai-subset": "1.4.0",
    "eslint": "3.13.1",
    "eslint-config-airbnb-base": "11.0.1",
    "eslint-plugin-import": "2.2.0",
    "file-loader": "0.10.1",
    "webpack": "2.3.1"
  }
}
