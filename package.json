{
  "name": "superbowleto",
  "version": "1.0.0",
  "description": "A simple microservice to issue, register and manage boletos",
  "main": "index.js",
  "private": true,
  "scripts": {
    "build-dev": "webpack --config ./webpack/webpackfile.dev.js",
    "build-production": "webpack --config ./webpack/webpackfile.production.js",
    "build-test": "webpack --config ./webpack/webpackfile.test.js",
    "lint": "eslint 'src/**/*.js' 'test/**/*.js'",
    "setup": "node -r babel-register ./script/setup",
    "test": "ava -T=10s --verbose 'build/test/**/*.js'",
    "test-all": "ava -T=10s 'build/main.js'",
    "test-functional": "ava -T=10s --verbose 'build/test/functional/**/*.js'",
    "test-integration": "ava -T=10s --verbose 'build/test/integration/**/*.js'",
    "test-unit": "ava -T=10s --verbose 'build/test/unit/**/*.js'",
    "pretest": "yarn run setup && yarn run build-test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pagarme/superbowleto.git"
  },
  "keywords": [
    "boleto",
    "microservice"
  ],
  "author": "Pagar.me Pagamentos S/A <@pagarme>",
  "bugs": {
    "url": "https://github.com/pagarme/superbowleto/issues"
  },
  "homepage": "https://github.com/pagarme/superbowleto#readme",
  "dependencies": {
    "aws-sdk": "2.36.0",
    "axios": "0.16.0",
    "bluebird": "3.5.0",
    "cuid": "1.3.8",
    "joi": "10.4.1",
    "moment": "2.18.1",
    "pg": "6.1.5",
    "ramda": "0.23.0",
    "sequelize": "3.30.4",
    "serverless": "1.10.2",
    "sqs-quooler": "1.3.0"
  },
  "devDependencies": {
    "@ava/babel-preset-stage-4": "1.0.0",
    "ava": "0.18.2",
    "babel-loader": "6.4.1",
    "babel-plugin-transform-builtin-extend": "1.1.2",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-es2015": "6.24.0",
    "chai": "3.5.0",
    "chai-subset": "1.5.0",
    "eslint": "3.19.0",
    "eslint-config-airbnb-base": "11.1.2",
    "eslint-plugin-import": "2.2.0",
    "file-loader": "0.11.1",
    "source-map-support": "0.4.14",
    "webpack": "2.3.3"
  }
}
